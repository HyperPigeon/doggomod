{
  "schemaVersion": 1,
  "id": "doggomod",
  "version": "${version}",

  "name": "Doggo Mod",
  "description": "Gives Minecraft's dogs (Tamed wolves) more things to do. Also adds dog bowls so dogs can eat whenever they want and a little tennis ball to play with.",
  "authors": [
    "MrKacafirekCZ"
  ],
  "contact": {
    "homepage": "https://github.com/MrKacafirekCZ/doggomod",
    "sources": ""
  },

  "license": "All rights reserved",
  "icon": "assets/doggomod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "mrkacafirekcz.doggomod.DoggoMod"
    ],
    "client": [
      "mrkacafirekcz.doggomod.DoggoModClient"
    ],
    "server": [
      "mrkacafirekcz.doggomod.DoggoModServer"
    ]
  },
  "mixins": [
    "doggomod.mixins.json"
  ],

  "depends": {
    "fabric-item-api-v1": "*",
    "fabric-item-groups-v0": "*",
    "fabric-networking-v0": "*",
    "fabric-object-builder-api-v1": "*",
    "fabric-renderer-registries-v1": "*",
    "fabric-screen-handler-api-v1": "*",
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "another-mod": "*"
  }
}
